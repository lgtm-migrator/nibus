{
  "name": "@nibus/mibs",
  "version": "3.5.4",
  "keywords": [
    "nata-info",
    "nibus",
    "sarakusha"
  ],
  "author": "Andrei Sarakeev <sarakusha@gmail.com>",
  "homepage": "https://github.com/sarakusha/nibus/packages/mibs#readme",
  "license": "MIT",
  "main": "index.js",
  "types": "index.d.ts",
  "module": "index.mjs",
  "files": [
    "json",
    "index.js",
    "index.d.js",
    "index.mjs",
    "!**/*.spec.*",
    "EULA.md",
    "CHANGELOG.md",
    "README.md"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/sarakusha/nibus.git",
    "directory": "packages/mibs"
  },
  "dependencies": {
    "fp-ts": "^2.11.10",
    "io-ts": "^2.2.16"
  },
  "scripts": {
    "build": "yarn build:module && yarn rename && yarn build:main",
    "build:main": "tsc --inlineSourceMap --declaration index.ts",
    "build:module": "tsc --inlineSourceMap --moduleResolution node --target es2021 index.ts",
    "rename": "rimraf index.mjs && node -e \"require('fs').rename('index.js', 'index.mjs', err => err && console.error(err.message))\"",
    "yalc:push": "yalc push"
  }
}
